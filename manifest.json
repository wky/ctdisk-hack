{
    "manifest_version": 2,
    "name": "CTdisk hack",
    "description": "block ads, straight to download",
    "version": "0.2",
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": ["http://*/*", "tabs", "debugger", 
    "webRequest", "webRequestBlocking", "contentSettings"],
    "browser_action": {
        "default_icon":"favicon.ico",
        "default_popup":"popup.html"
    }
}